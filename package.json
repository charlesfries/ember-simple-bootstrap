    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-typescript": "^5.1.0",
    "ember-composable-helpers": "^5.0.0",
    "ember-event-helpers": "^0.1.1",
    "ember-modifier": "^4.1.0",
    "ember-truth-helpers": "^3.0.0"
  },
  "devDependencies": {
